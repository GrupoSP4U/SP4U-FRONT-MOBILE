<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/Fund">

    <TextView
        android:id="@+id/tv_titulo_cadastro_empresa"
        android:layout_width="193dp"
        android:layout_height="28dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:fontFamily="@font/montserrat"
        android:text="cadastro"
        tools:layout_editor_absoluteY="60dp"
        android:textSize="18sp"
        android:background="@color/White"
        android:textAlignment="center"
        />

    <ImageView
        android:id="@+id/img_empresa"
        android:layout_width="95dp"
        android:layout_height="126dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_titulo_cadastro_empresa"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="36dp"
        android:src="@drawable/icon_company"
        />

    <TextView
    android:id="@+id/tv_razao_social"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_constraintTop_toBottomOf="@id/img_empresa"
    android:fontFamily="@font/montserrat"
    android:layout_marginTop="40dp"
    android:text="razão social"
    android:textColor="@color/White"
    android:textSize="16sp"
    tools:layout_editor_absoluteX="66dp"
    />

    <EditText
        android:id="@+id/et_razao_social"
        android:layout_width="280dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_razao_social"
        android:fontFamily="@font/montserrat"
        android:hint="Digite a razão social"
        android:layout_marginTop="8sp"
        android:background="@drawable/et_style_orange"
        android:textColorHint="@color/White"
        android:inputType="text"
        />

    <TextView
        android:id="@+id/tv_nome_fantasia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/et_razao_social"
        android:fontFamily="@font/montserrat"
        android:layout_marginTop="15dp"
        android:text="nome fantasia"
        android:textColor="@color/White"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="66dp"
        />

    <EditText
        android:id="@+id/et_nome_fantasia"
        android:layout_width="280dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_nome_fantasia"
        android:fontFamily="@font/montserrat"
        android:hint="Digite o nome fantasia"
        android:layout_marginTop="8sp"
        android:background="@drawable/et_style_orange"
        android:textColorHint="@color/White"
        android:inputType="text"
        />


    <TextView
        android:id="@+id/tv_telefone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/et_nome_fantasia"
        android:fontFamily="@font/montserrat"
        android:layout_marginTop="15dp"
        android:text="telefone"
        android:textColor="@color/White"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="66dp"
        />

    <EditText
        android:id="@+id/et_telefone"
        android:layout_width="280dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_telefone"
        android:fontFamily="@font/montserrat"
        android:hint="(XX) 00000-0000"
        android:layout_marginTop="8sp"
        android:background="@drawable/et_style_orange"
        android:textColorHint="@color/White"
        android:inputType="phone"
        />

    <TextView
        android:id="@+id/tv_cnpj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/et_telefone"
        android:fontFamily="@font/montserrat"
        android:layout_marginTop="15dp"
        android:text="cnpj"
        android:textColor="@color/White"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="66dp"
        />

    <EditText
        android:id="@+id/et_cnpj"
        android:layout_width="280dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_cnpj"
        android:fontFamily="@font/montserrat"
        android:hint="(XX) 00000-0000"
        android:layout_marginTop="8sp"
        android:background="@drawable/et_style_orange"
        android:textColorHint="@color/White"
        android:inputType="phone"
        />

    <ImageView
        android:id="@+id/img_next"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintTop_toBottomOf="@id/et_cnpj"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="27dp"
        android:src="@drawable/icon_next"
        tools:layout_editor_absoluteX="315dp"
        android:onClick="goToRegisterEstablishmentAdress"
        />

</androidx.constraintlayout.widget.ConstraintLayout>